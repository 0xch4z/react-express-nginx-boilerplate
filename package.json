{
  "name": "react-express-nginx-boilerplate",
  "version": "0.0.1",
  "description": "A boilerplate for isomorphic web applications with react, express and nginx.",
  "main": "index.js",
  "scripts": {
    "bootstrap": "sudo bash ./bin/bootstrap.sh",
    "start": "./node_modules/.bin/pm2 start ./bin/start.sh",
    "stop": "./node_modules/.bin/pm2 stop all",
    "monitor": "./node_modules/.bin/pm2 monitor",
    "restart": "bash ./bin/restart.sh",
    "list": "bash ./bin/list.sh",
    "build": "bash ./bin/build.sh",
    "dev": "bash ./bin/dev.sh",
    "deps": "bash ./bin/deps.sh",
    "rmdeps": "bash ./bin/rmdeps.sh",
    "clean": "bash ./bin/clean.sh"
  },
  "author": "Charles Kenney",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^3.5.0",
    "forever": "^0.15.3",
    "pm2": "^2.6.1"
  }
}
